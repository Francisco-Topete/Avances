@model Pagina.Models.Tipo

@{
    ViewBag.Title = "Borrar Tipo";
}
<h3 class="titulo subtitulo" style="color: black;">Seguro que quieres borrar esto?</h3>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Nombre)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Nombre)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Borrar" class="boton-crear" />
        </div>
    }
</div>
